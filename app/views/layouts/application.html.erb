<!DOCTYPE html>
<html>
<head>
  <title>Moviesapp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="banner">
	<%= image_tag("rails.png", :size => "100x50") %>
	<%= @page_title || "Movies We Love" %>
	
</div>

</div>

<div id="nav">
	<ul>
	  <li><%= link_to "Questions"%></li>
      <li><%= link_to "Movie News", 'http://oscar.go.com/nominees' %></li>
      <li><%= link_to "Movies", movies_path%></li>
    </ul>
</div>

<%= yield %>

<div id="banner">
	<%= image_tag("rails.png", :size => "100x50") %>
	<%= @page_title || "We Do Love Them Movies" %>
	



</body>
</html>
